{
  "name": "tanstack-start-template",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "license": "MIT",
  "author": "Daniel Yeoman",
  "version": "1.0.0",
  "description": "TanStack Start Template built with TanStack Start, Better Auth, Convex, Tailwind CSS, Shadcn/UI, and Resend",
  "homepage": "https://github.com/dyeoman2/tanstack-start-template",
  "repository": {
    "type": "git",
    "url": "https://github.com/dyeoman2/tanstack-start-template.git"
  },
  "engines": {
    "node": ">=24"
  },
  "keywords": [
    "tanstack start",
    "react",
    "typescript",
    "starter",
    "template",
    "fullstack",
    "authentication",
    "convex",
    "resend",
    "netlify"
  ],
  "scripts": {
    "dev": "concurrently -n vite,convex -c blue,green \"NODE_OPTIONS='--import ./instrument.server.mjs' vite dev\" \"npx convex dev\"",
    "dev:full": "tsx scripts/dev-full.ts",
    "build": "vite build && mkdir -p .output/server && cp instrument.server.mjs .output/server",
    "start": "node --import ./.output/server/instrument.server.mjs ./.output/server/index.mjs",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "check": "biome check .",
    "fix": "biome check --write .",
    "knip": "knip",
    "knip:prod": "knip --production",
    "setup:env": "tsx scripts/setup-env.ts",
    "setup:convex": "tsx scripts/setup-convex.ts",
    "setup:dev": "tsx scripts/setup-dev.ts",
    "setup:prod": "tsx scripts/setup-prod.ts",
    "precommit": "biome check --write",
    "updates": "npx npm-check-updates",
    "analyze": "pnpm build && npx vite-bundle-analyzer dist/client/assets/*.js",
    "infra:deploy": "tsx infra/storage.ts",
    "infra:preview": "tsx infra/storage.ts --preview"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.927.0",
    "@convex-dev/better-auth": "^0.9.7",
    "@convex-dev/rate-limiter": "^0.2.14",
    "@convex-dev/resend": "^0.1.13",
    "@mendable/firecrawl-js": "^4.5.0",
    "@netlify/vite-plugin-tanstack-start": "^1.1.11",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@sentry/profiling-node": "^10.23.0",
    "@sentry/tanstackstart-react": "^10.23.0",
    "@tanstack/react-form": "^1.23.8",
    "@tanstack/react-router": "^1.134.13",
    "@tanstack/react-router-devtools": "^1.134.13",
    "@tanstack/react-start": "^1.134.14",
    "@tanstack/react-table": "^8.21.3",
    "@useautumn/convex": "^0.0.14",
    "ai": "^5.0.89",
    "autumn-js": "^0.1.46",
    "better-auth": "^1.3.34",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.28.2",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.553.0",
    "next-themes": "^0.4.6",
    "npm-check-updates": "^19.1.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.6",
    "vite": "7.2.2",
    "workers-ai-provider": "^2.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@aws-sdk/client-iam": "^3.927.0",
    "@biomejs/biome": "^2.3.4",
    "@tailwindcss/vite": "^4.1.17",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "concurrently": "^9.1.2",
    "knip": "^5.68.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
